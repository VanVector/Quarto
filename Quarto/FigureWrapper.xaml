<UserControl x:Class="Quarto.FigureWrapper"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" Height="50" Width="50">
    <Grid Width="50" Height="50">
        <Rectangle Height="50" HorizontalAlignment="Left" Name="FigureRectangle" Stroke="#FFCCCCCC" VerticalAlignment="Top" Width="50" Fill="#FFDDDDDD" RadiusY="2" RadiusX="2" StrokeThickness="2"></Rectangle>
        <Ellipse Name="Ellipse1" Height="12" HorizontalAlignment="Left" Stroke="#FFAAAAAA" VerticalAlignment="Top" Width="12" Margin="6,14,0,0" Fill="White" />
        <Ellipse Name="Ellipse2" Height="12" HorizontalAlignment="Left" Margin="24,6,0,0" Stroke="#FFAAAAAA" VerticalAlignment="Top" Width="12" Fill="White" />
        <Ellipse Name="Ellipse3" Height="12" HorizontalAlignment="Left" Margin="14,32,0,0" Stroke="#FFAAAAAA" VerticalAlignment="Top" Width="12" Fill="White" />
        <Ellipse Name="Ellipse4" Height="12" HorizontalAlignment="Left" Margin="32,24,0,0" Stroke="#FFAAAAAA" VerticalAlignment="Top" Width="12" Fill="#FF444444" />
    </Grid>
</UserControl>
